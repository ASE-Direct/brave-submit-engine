# Supabase Configuration
# Get these values from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
VITE_SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Resend API Configuration
# Get your API key from: https://resend.com/api-keys
RESEND_API_KEY=your_resend_api_key_here

# Supabase Access Token (for CLI operations)
# Get from: https://supabase.com/dashboard/account/tokens
SUPABASE_ACCESS_TOKEN=your_supabase_access_token_here

# Notification Settings
NOTIFICATION_EMAIL=your_notification_email@company.com

# Google reCAPTCHA (optional, for production)
VITE_RECAPTCHA_SITE_KEY=your_recaptcha_site_key_here
VITE_RECAPTCHA_SECRET_KEY=your_recaptcha_secret_key_here

